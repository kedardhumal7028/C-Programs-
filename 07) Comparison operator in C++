#include<iostream>
using namespace std;
int main()
{
  int a=4, b=5;
  cout<<"Operators in C++"<<endl;
  
  cout<<"Following are the Comparison Operator in C++"<<endl;
  
  //Comparison Operators
  
  cout<<"The value of a==b is"<<(a==b)<<endl;
  cout<<"The value of a!=b is"<<(a!=b)<<endl;
  cout<<"The value of a>=b is"<<(a>=b)<<endl;
  cout<<"The value of a<=b is"<<(a<=b)<<endl;
  cout<<"The value of a>b1 is"<<(a>b)<<endl;
  cout<<"The value of a<b1 is"<<(a<b)<<endl;
}  
