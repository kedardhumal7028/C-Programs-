//Change variable value 

#include<iostream>
using namespace std;
int main()
{
  int a=34;
  cout<<"The value of a was:"<<a<<endl;
  a=12;
  cout<<"The value of a was:"<<a<<endl;
  return 0;
}  


             //Or
             
             

//Do not change variable value 

#include<iostream>
using namespace std;
int main()
{
 const int a=34;
  cout<<"The value of a was:"<<a<<endl;
  a=12;
  cout<<"The value of a was:"<<a<<endl;
  return 0;
}  
